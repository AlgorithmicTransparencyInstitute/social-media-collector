  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "prettier": "prettier --write '**/*.{js,json,md}'",
    "prebuild": "rimraf build",
    "build": "webpack --progress",
    "watch": "webpack --watch --progress",
    "backend-test": "backend-tests/local_server.py",
    "prefirefox": "npm run build  -- --env.config=std --env.browser=firefox --env.build=debug",
    "firefox": "./node_modules/.bin/web-ext run",
    "prechrome": "npm run build -- --env.config=std --env.browser=chrome --env.build=debug",
    "chrome": "npx web-ext run -t chromium -c ./web-ext-chromium-config.js",
    "test": "TZ=UTC jest --env=jsdom",
    "wintest": "set TZ=UTC && jest --env=jsdom",
    "lint": "eslint .",
    "stylelint": "stylelint \"./src/**/*.less\""
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "proseWrap": "never",
    "arrowParens": "avoid",
    "trailingComma": "none",
    "printWidth": 100
  },
  "lint-staged": {
    "**/*.{js,json,md}": [
      "prettier --write"
    ],
    "**/*.js": [
      "eslint ."
    ],
    "./src/**/*.less": [
      "stylelint"
    ]
  }
